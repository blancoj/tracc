<p id="notice"><%= notice %></p>

<h4> Import Assessment Data </h4>

<%= form_tag("import_assessment", :enctype =>"multipart/form-data") do %>
   <div class="row">
   <div class="field">
     <%= label_tag :file %>
     <%= file_field_tag :file,  :required => true  %>
   </div>
</div>
<% cname = Cycle.all %>
<div class="row">
    <div class="field">
     <%= label_tag :cycle %>
     <%= select_tag "cycle", options_from_collection_for_select(cname, "cycle_name", "cycle_name")  %>
  
   </div>
</div>
   <div class="row">
   <div class="actions">
      <%= submit_tag 'Next' %>
   </div>
</div>
<% end %>